# AEIMS .gitignore

# Logs and temporary files
logs/
*.log
*.tmp
*.temp

# Environment and configuration (sensitive)
.env
.env.local
.env.production
.env.staging
*.env

# Database files
*.db
*.sqlite
*.sqlite3
database/*.db

# Cache and session files
cache/
sessions/
tmp/
temp/

# Upload directories (user content)
uploads/
files/

# Vendor dependencies (if using Composer in future)
vendor/
composer.lock

# Node modules (if using npm/yarn in future)
node_modules/
npm-debug.log
yarn-error.log
package-lock.json

# IDE and editor files
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store
Thumbs.db

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Deployment and build artifacts
dist/
build/
*.tar.gz
*.zip

# Terraform state files (sensitive)
infrastructure/terraform/*.tfstate
infrastructure/terraform/*.tfstate.backup
infrastructure/terraform/.terraform/
infrastructure/terraform/*.tfvars
infrastructure/terraform/.terraform.lock.hcl

# All Terraform provider directories (large binary files)
terraform/.terraform/
*/.terraform/
**/.terraform/

# Docker build context exclusions
.dockerignore

# Sensitive files
config/production.php
config/staging.php
*.key
*.pem
*.p12
*.crt

# Rate limiting data (contains IPs)
data/rate-limit.json

# Backup files
*.bak
*.backup
*.old

# Local development overrides
local.php
dev.php
test.php

# PHPUnit and testing
phpunit.xml
.phpunit.result.cache
tests/_output/
tests/_support/_generated

# Claude AI files (optional - remove if you want to track)
.claude/